
CC   := gcc
SRC  := .c
OBJ  := .o
NAME := holberton
RM: rm -f

all: main$(OBJ) holberton$(OBJ) m.h
	$(CC) main$(OBJ) holberton$(OBJ) -o $(NAME)

main$(OBJ): main$(SRC)
	$(CC) -c -o main$(OBJ) main$(SRC)

holberton$(OBJ): holberton$(SRC)
	$(CC) -c -o holberton$(OBJ) holberton$(SRC)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) main$(OBJ) holberton$(OBJ)

fclean: clean oclean

re: oclean all
